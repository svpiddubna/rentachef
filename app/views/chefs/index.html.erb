<div class='container'>
  <div class='row'>
    <div class="search-page">
      <h1>Choose your chef in <%= params[:location].capitalize %></h1>
      <% @chefs.each do |chef| %>
        <div class="col-xs-12 col-md-6">
          <%= render 'chefs/card', chef: chef %>
        </div>
      <% end %>
    </div>
  </div>
</div>
